<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <layer-list>
            <item>
                <bitmap android:gravity="left|center_vertical" android:src="@drawable/meun_sidebar_about_icon_sel" />
            </item>
            <item>
                <bitmap android:gravity="right|center_vertical" android:src="@drawable/meun_sidebar_icon_arrow" />
            </item>
        </layer-list>
    </item>
    <item android:state_pressed="true">
        <layer-list>
            <item>
                <bitmap android:gravity="left|center_vertical" android:src="@drawable/meun_sidebar_about_icon_sel" />
            </item>
            <item>
                <bitmap android:gravity="right|center_vertical" android:src="@drawable/meun_sidebar_icon_arrow" />
            </item>
        </layer-list>
    </item>

    <item>
        <layer-list>
            <item>
                <bitmap android:gravity="left|center_vertical" android:src="@drawable/meun_sidebar_about_icon_nor" />
            </item>
        </layer-list>
    </item>
</selector>