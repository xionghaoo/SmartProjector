<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bluetooth_bg"
    android:clipToPadding="false">
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/project_image_choose_rcv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clipToPadding="false"
        android:overScrollMode="never"
        android:layout_marginTop="@dimen/ubt_dimen_142px"
        android:layout_marginBottom="@dimen/ubt_dimen_142px"
        android:paddingLeft="@dimen/ubt_dimen_104px"
        android:paddingStart="@dimen/ubt_dimen_104px"
        android:paddingEnd="@dimen/ubt_dimen_104px"
        android:paddingRight="@dimen/ubt_dimen_104px">

    </androidx.recyclerview.widget.RecyclerView>
    <Button
        android:id="@+id/project_image_choose_back_btn"
        android:layout_width="@dimen/ubt_dimen_128px"
        android:layout_height="@dimen/ubt_dimen_128px"
        android:layout_marginLeft="@dimen/ubt_dimen_20px"
        android:layout_marginStart="@dimen/ubt_dimen_20px"
        android:background="@drawable/ic_tab_back"/>
</RelativeLayout>