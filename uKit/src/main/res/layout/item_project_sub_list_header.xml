<?xml version="1.0" encoding="utf-8"?>


<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/project_sub_list_add_new_btn"
    android:layout_width="match_parent"
    android:layout_height="@dimen/ubt_dimen_126px"
    android:layout_marginStart="@dimen/ubt_dimen_86px"
    android:layout_marginLeft="@dimen/ubt_dimen_86px"
    android:layout_marginEnd="@dimen/ubt_dimen_86px"
    android:layout_marginRight="@dimen/ubt_dimen_86px"
    android:background="@drawable/selector_project_sub_list_new_btn"
    android:gravity="center">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <View
            android:layout_width="@dimen/ubt_dimen_70px"
            android:layout_height="@dimen/ubt_dimen_70px"
            android:background="@drawable/popup_list_new_icon" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/ubt_dimen_12px"
            android:textColor="@android:color/white"
            android:textSize="@dimen/ubt_font_46px"
            android:text="@string/project_sub_list_add_new" />
    </LinearLayout>

</RelativeLayout>
