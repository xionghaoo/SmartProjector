<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:clickable="true"
    android:orientation="vertical"
    android:gravity="center_horizontal">

    <View
        android:id="@+id/ai_background"
        android:layout_width="@dimen/ubt_dimen_690px"
        android:layout_height="@dimen/ubt_dimen_576px"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/ubt_dimen_224px"
        android:background="@drawable/img_noresources"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/ubt_dimen_28px"
        android:gravity="center"
        android:textSize="@dimen/ubt_font_46px"
        android:textColor="#555B67"
        android:text="@string/ai_developing"/>
</LinearLayout>