<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="@dimen/_140dp"
    tools:context=".ui.infant.InfantMainFragment">

    <FrameLayout
        android:id="@+id/textView28"
        android:layout_width="@dimen/_519dp"
        android:layout_height="@dimen/_514dp"
        android:layout_marginStart="@dimen/_51dp"
        android:layout_marginTop="@dimen/_24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" >
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@mipmap/ic_infant_thinking_left_1"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/_26sp"
            android:textColor="@color/white"
            android:layout_marginBottom="@dimen/_30dp"
            android:layout_gravity="bottom|center_horizontal"
            android:text="思维逻辑"/>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/textView67"
        android:layout_width="@dimen/_300dp"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/_29dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView28"
        app:layout_constraintStart_toEndOf="@+id/textView28"
        app:layout_constraintTop_toTopOf="@+id/textView28" >
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@mipmap/ic_infant_thinking_right_1"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/_26sp"
            android:textColor="@color/white"
            android:layout_marginBottom="@dimen/_30dp"
            android:layout_gravity="bottom|center_horizontal"
            android:text="空间想象游戏"/>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/textView69"
        android:layout_width="@dimen/_300dp"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/_20dp"
        android:layout_marginEnd="@dimen/_51dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView67"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView67"
        app:layout_constraintTop_toTopOf="@+id/textView67" >
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@mipmap/ic_infant_interest_right_2"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/_26sp"
            android:textColor="@color/white"
            android:layout_marginBottom="@dimen/_30dp"
            android:layout_gravity="bottom|center_horizontal"
            android:text="推理游戏"/>
    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>