<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="@dimen/_140dp"
    tools:context=".ui.MainFragment">

    <TextView
        android:id="@+id/textView20"
        android:layout_width="@dimen/_20dp"
        android:layout_height="@dimen/_36dp"
        android:layout_marginStart="@dimen/_147dp"
        android:layout_marginBottom="@dimen/_255dp"
        android:background="@drawable/shape_connect_line_bottom"
        app:layout_constraintBottom_toBottomOf="@+id/card_assistant_learn_individuation"
        app:layout_constraintStart_toStartOf="@+id/card_assistant_learn_individuation" />

    <TextView
        android:id="@+id/textView45"
        android:layout_width="@dimen/_20dp"
        android:layout_height="@dimen/_36dp"
        android:layout_marginEnd="@dimen/_147dp"
        android:layout_marginBottom="@dimen/_255dp"
        android:background="@drawable/shape_connect_line_bottom"
        app:layout_constraintBottom_toBottomOf="@+id/card_assistant_learn_individuation"
        app:layout_constraintEnd_toEndOf="@+id/card_assistant_famous_teacher_classroom" />

    <FrameLayout
        android:id="@+id/card_assistant_learn_individuation"
        android:layout_width="@dimen/_314dp"
        android:layout_height="@dimen/_265dp"
        android:layout_marginTop="@dimen/_11dp"
        android:background="@mipmap/ic_assistant_learn_individuation"
        app:layout_constraintStart_toStartOf="@+id/card_assistant_famous_teacher_classroom"
        app:layout_constraintTop_toBottomOf="@+id/card_assistant_famous_teacher_classroom">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:layout_marginBottom="@dimen/_9dp"
            android:text="个性化学习"
            android:textColor="@color/white"
            android:textSize="@dimen/_26sp" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/card_assistant_finger_read"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/_8dp"
        android:background="@mipmap/ic_assistant_finger_read"
        app:layout_constraintBottom_toBottomOf="@+id/card_assistant_learn_individuation"
        app:layout_constraintEnd_toEndOf="@+id/card_assistant_famous_teacher_classroom"
        app:layout_constraintStart_toEndOf="@+id/card_assistant_learn_individuation"
        app:layout_constraintTop_toTopOf="@+id/card_assistant_learn_individuation" >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:layout_marginBottom="@dimen/_9dp"
            android:text="指尖点读"
            android:textColor="@color/white"
            android:textSize="@dimen/_26sp" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/card_assistant_famous_teacher_classroom"
        android:layout_width="@dimen/_636dp"
        android:layout_height="@dimen/_261dp"
        android:layout_marginStart="@dimen/_75dp"
        android:layout_marginTop="@dimen/_21dp"
        android:background="@mipmap/ic_assistant_classroom"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/_26sp"
            android:textColor="@color/white"
            android:layout_gravity="bottom"
            android:layout_marginBottom="@dimen/_25dp"
            android:layout_marginStart="@dimen/_37dp"
            android:text="名师课堂"/>
    </FrameLayout>

    <LinearLayout
        android:id="@+id/textView41"
        android:layout_width="@dimen/_286dp"
        android:layout_height="@dimen/_548dp"
        android:layout_marginStart="@dimen/_32dp"
        android:background="@mipmap/ic_assistant_special_container"
        android:orientation="vertical"
        android:paddingStart="@dimen/_18dp"
        android:paddingEnd="@dimen/_32dp"
        app:layout_constraintStart_toEndOf="@+id/card_assistant_famous_teacher_classroom"
        app:layout_constraintTop_toTopOf="parent">

        <FrameLayout
            android:id="@+id/btn_assistant_chinese"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_120dp"
            android:layout_marginTop="@dimen/_28dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@mipmap/ic_assistant_chinese" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_46dp"
                android:layout_gravity="start|top"
                android:layout_marginTop="@dimen/_43dp"
                android:maxLines="1"
                android:singleLine="true"
                android:text="语文"
                android:textColor="@color/white"
                android:textSize="@dimen/_26sp"
                android:textStyle="bold" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/btn_assistant_english"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_120dp"
            android:layout_marginTop="@dimen/_8dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@mipmap/ic_assistant_english" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start|top"
                android:layout_marginStart="@dimen/_46dp"
                android:layout_marginTop="@dimen/_43dp"
                android:maxLines="1"
                android:singleLine="true"
                android:text="英语"
                android:textColor="@color/white"
                android:textSize="@dimen/_26sp"
                android:textStyle="bold" />

        </FrameLayout>

        <FrameLayout
            android:id="@+id/btn_assistant_math"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_120dp"
            android:layout_marginTop="@dimen/_8dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@mipmap/ic_assistant_mathematics" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start|top"
                android:layout_marginStart="@dimen/_46dp"
                android:layout_marginTop="@dimen/_43dp"
                android:maxLines="1"
                android:singleLine="true"
                android:text="数学"
                android:textColor="@color/white"
                android:textSize="@dimen/_26sp"
                android:textStyle="bold" />

        </FrameLayout>

        <FrameLayout
            android:id="@+id/btn_assistant_morals"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_120dp"
            android:layout_marginTop="@dimen/_8dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@mipmap/ic_assistant_legal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start|top"
                android:layout_marginStart="@dimen/_46dp"
                android:layout_marginTop="@dimen/_19dp"
                android:maxLines="1"
                android:singleLine="true"
                android:text="道德"
                android:textColor="@color/white"
                android:textSize="@dimen/_26sp"
                android:textStyle="bold" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start|bottom"
                android:layout_marginStart="@dimen/_46dp"
                android:layout_marginBottom="@dimen/_19dp"
                android:maxLines="1"
                android:singleLine="true"
                android:text="法制"
                android:textColor="@color/white"
                android:textSize="@dimen/_26sp"
                android:textStyle="bold" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start|center"
                android:layout_marginStart="@dimen/_64dp"
                android:maxLines="1"
                android:singleLine="true"
                android:text="与"
                android:textColor="@color/white"
                android:textSize="@dimen/_16sp"
                android:textStyle="bold" />

        </FrameLayout>

    </LinearLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/textView42"
        android:layout_width="@dimen/_148dp"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/_22dp"
        android:layout_marginEnd="@dimen/_75dp"
        android:paddingEnd="@dimen/_12dp"
        android:orientation="vertical"
        android:gravity="center_horizontal"
        android:background="@mipmap/ic_assistant_tools_container"
        app:layout_constraintBottom_toBottomOf="@+id/textView41"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView41"
        app:layout_constraintTop_toTopOf="@+id/textView41" >

        <ImageView
            android:id="@+id/textView46"
            android:layout_width="@dimen/_74dp"
            android:layout_height="@dimen/_74dp"
            android:layout_marginTop="@dimen/_53dp"
            android:src="@mipmap/ic_assistant_tools_answer"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:includeFontPadding="false"
            android:text="找答案"
            android:textStyle="bold"
            android:textColor="@color/white"
            android:textSize="@dimen/_24sp"
            android:layout_marginTop="@dimen/_160dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/textView47"
            android:layout_width="@dimen/_74dp"
            android:layout_height="@dimen/_74dp"
            android:layout_marginTop="@dimen/_228dp"
            android:src="@mipmap/ic_assistant_tools_query"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView48"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="查单词"
            android:textStyle="bold"
            android:textColor="@color/white"
            android:textSize="@dimen/_24sp"
            android:layout_marginTop="@dimen/_38dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView47" />

        <ImageView
            android:id="@+id/textView49"
            android:layout_width="@dimen/_74dp"
            android:layout_height="@dimen/_74dp"
            android:layout_marginBottom="@dimen/_77dp"
            android:src="@mipmap/ic_assistant_tools_advance"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView53"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_35dp"
            android:text="进阶学习"
            android:textStyle="bold"
            android:textColor="@color/white"
            android:textSize="@dimen/_24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView49" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>