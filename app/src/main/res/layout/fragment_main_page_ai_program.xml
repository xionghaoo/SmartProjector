<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="@dimen/_140dp"
    tools:context=".ui.MainFragment">

    <FrameLayout
        android:id="@+id/card_program_my_works"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/_8dp"
        android:background="@mipmap/ic_program_my_works"
        app:layout_constraintBottom_toBottomOf="@+id/card_program_do_work"
        app:layout_constraintEnd_toEndOf="@+id/card_program_teaching_video"
        app:layout_constraintStart_toEndOf="@+id/card_program_do_work"
        app:layout_constraintTop_toTopOf="@+id/card_program_do_work">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_marginStart="@dimen/_73dp"
            android:layout_marginBottom="@dimen/_18dp"
            android:text="我的作品"
            android:textColor="@color/white"
            android:textSize="@dimen/_26sp" />
    </FrameLayout>

    <TextView
        android:id="@+id/textView30"
        android:layout_width="@dimen/_20dp"
        android:layout_height="0dp"
        android:background="@drawable/shape_connect_line_bottom"
        app:layout_constraintBottom_toBottomOf="@+id/textView20"
        app:layout_constraintEnd_toEndOf="@+id/card_program_my_works"
        app:layout_constraintStart_toStartOf="@+id/card_program_my_works"
        app:layout_constraintTop_toTopOf="@+id/textView20" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="@dimen/_36dp"
        android:layout_height="@dimen/_20dp"
        android:layout_marginStart="@dimen/_254dp"
        android:background="@drawable/shape_connect_line_right"
        app:layout_constraintBottom_toBottomOf="@+id/card_program_do_work"
        app:layout_constraintStart_toStartOf="@+id/card_program_do_work"
        app:layout_constraintTop_toTopOf="@+id/card_program_do_work" />

    <FrameLayout
        android:id="@+id/card_program_do_work"
        android:layout_width="@dimen/_265dp"
        android:layout_height="@dimen/_250dp"
        android:layout_marginTop="@dimen/_15dp"
        android:background="@mipmap/ic_program_practise"
        app:layout_constraintStart_toStartOf="@+id/card_program_teaching_video"
        app:layout_constraintTop_toBottomOf="@+id/card_program_teaching_video">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_marginStart="@dimen/_73dp"
            android:layout_marginBottom="@dimen/_18dp"
            android:text="动手编程"
            android:textColor="@color/white"
            android:textSize="@dimen/_26sp" />
    </FrameLayout>

    <TextView
        android:id="@+id/textView20"
        android:layout_width="@dimen/_20dp"
        android:layout_height="@dimen/_36dp"
        android:layout_marginBottom="@dimen/_246dp"
        android:background="@drawable/shape_connect_line_bottom"
        app:layout_constraintBottom_toBottomOf="@+id/card_program_do_work"
        app:layout_constraintEnd_toEndOf="@+id/card_program_do_work"
        app:layout_constraintStart_toStartOf="@+id/card_program_do_work" />

    <FrameLayout
        android:id="@+id/card_program_teaching_video"
        android:layout_width="@dimen/_538dp"
        android:layout_height="@dimen/_249dp"
        android:layout_marginStart="@dimen/_75dp"
        android:layout_marginTop="@dimen/_29dp"
        android:background="@mipmap/ic_chinese_prepare"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_marginStart="@dimen/_73dp"
            android:layout_marginBottom="@dimen/_18dp"
            android:text="教学视频"
            android:textColor="@color/white"
            android:textSize="@dimen/_26sp" />
    </FrameLayout>

    <LinearLayout
        android:id="@+id/textView41"
        android:layout_width="@dimen/_346dp"
        android:layout_height="@dimen/_548dp"
        android:layout_marginStart="@dimen/_51dp"
        android:background="@mipmap/ic_english_special_container_bg"
        android:orientation="vertical"
        android:paddingStart="@dimen/_18dp"
        android:paddingEnd="@dimen/_32dp"
        app:layout_constraintStart_toEndOf="@+id/card_program_teaching_video"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/btn_program_thinking"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_120dp"
            android:layout_marginTop="@dimen/_28dp"
            android:background="@drawable/shape_round_rect_white_alpha2"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="@dimen/_76dp"
                android:layout_height="@dimen/_76dp"
                android:layout_marginStart="@dimen/_23dp"
                android:src="@mipmap/ic_program_thinking" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24dp"
                android:layout_marginEnd="@dimen/_15dp"
                android:maxLines="1"
                android:singleLine="true"
                android:text="少儿编程思维"
                android:textColor="@color/white"
                android:textSize="@dimen/_26sp"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btn_program_scratch"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_120dp"
            android:layout_marginTop="@dimen/_8dp"
            android:background="@drawable/shape_round_rect_white_alpha2"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="@dimen/_76dp"
                android:layout_height="@dimen/_76dp"
                android:layout_marginStart="@dimen/_23dp"
                android:src="@mipmap/ic_program_scratch" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24dp"
                android:layout_marginEnd="@dimen/_15dp"
                android:maxLines="1"
                android:singleLine="true"
                android:text="趣味Scratch"
                android:textColor="@color/white"
                android:textSize="@dimen/_26sp"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btn_program_python"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_120dp"
            android:layout_marginTop="@dimen/_8dp"
            android:background="@drawable/shape_round_rect_white_alpha2"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="@dimen/_76dp"
                android:layout_height="@dimen/_76dp"
                android:layout_marginStart="@dimen/_23dp"
                android:src="@mipmap/ic_program_python" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24dp"
                android:layout_marginEnd="@dimen/_15dp"
                android:maxLines="1"
                android:singleLine="true"
                android:text="智能Python"
                android:textColor="@color/white"
                android:textSize="@dimen/_26sp"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btn_program_competition"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_120dp"
            android:layout_marginTop="@dimen/_8dp"
            android:background="@drawable/shape_round_rect_white_alpha2"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="@dimen/_76dp"
                android:layout_height="@dimen/_76dp"
                android:layout_marginStart="@dimen/_23dp"
                android:src="@mipmap/ic_program_competition" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24dp"
                android:layout_marginEnd="@dimen/_15dp"
                android:maxLines="1"
                android:singleLine="true"
                android:text="编程竞赛"
                android:textColor="@color/white"
                android:textSize="@dimen/_26sp"
                android:textStyle="bold" />
        </LinearLayout>

    </LinearLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/textView42"
        android:layout_width="@dimen/_148dp"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/_22dp"
        android:layout_marginEnd="@dimen/_76dp"
        android:orientation="vertical"
        android:paddingRight="@dimen/_14dp"
        android:gravity="center_horizontal"
        android:background="@mipmap/ic_assistant_tools_container"
        app:layout_constraintBottom_toBottomOf="@+id/textView41"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView41"
        app:layout_constraintTop_toTopOf="@+id/textView41" >

        <ImageView
            android:id="@+id/textView46"
            android:layout_width="@dimen/_64dp"
            android:layout_height="@dimen/_64dp"
            android:layout_marginTop="@dimen/_53dp"
            android:src="@mipmap/ic_program_community"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:includeFontPadding="false"
            android:text="交流社区"
            android:textStyle="bold"
            android:textColor="@color/white"
            android:textSize="@dimen/_24sp"
            android:layout_marginTop="@dimen/_160dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/textView47"
            android:layout_width="@dimen/_64dp"
            android:layout_height="@dimen/_64dp"
            android:layout_marginTop="@dimen/_233dp"
            android:src="@mipmap/ic_program_steam"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView48"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Steam教具"
            android:textStyle="bold"
            android:textColor="@color/white"
            android:textSize="@dimen/_24sp"
            android:layout_marginTop="@dimen/_38dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView47" />

        <ImageView
            android:id="@+id/textView49"
            android:layout_width="@dimen/_64dp"
            android:layout_height="@dimen/_64dp"
            android:layout_marginBottom="@dimen/_83dp"
            android:src="@mipmap/ic_program_solution"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView53"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_35dp"
            android:text="疑难解答"
            android:textStyle="bold"
            android:textColor="@color/white"
            android:textSize="@dimen/_24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView49" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>