<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="@dimen/_140dp"
    tools:context=".ui.MainFragment">

    <FrameLayout
        android:id="@+id/card_english_after_school"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/_8dp"
        android:background="@mipmap/ic_chinese_after_school"
        app:layout_constraintBottom_toBottomOf="@+id/card_english_classic"
        app:layout_constraintEnd_toEndOf="@+id/card_english_prepare_read"
        app:layout_constraintStart_toEndOf="@+id/card_english_classic"
        app:layout_constraintTop_toTopOf="@+id/card_english_classic">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_marginStart="@dimen/_73dp"
            android:layout_marginBottom="@dimen/_18dp"
            android:text="课后练习"
            android:textColor="@color/white"
            android:textSize="@dimen/_26sp" />
    </FrameLayout>

    <TextView
        android:id="@+id/textView30"
        android:layout_width="@dimen/_20dp"
        android:layout_height="0dp"
        android:background="@drawable/shape_connect_line_bottom"
        app:layout_constraintBottom_toBottomOf="@+id/textView20"
        app:layout_constraintEnd_toEndOf="@+id/card_english_after_school"
        app:layout_constraintStart_toStartOf="@+id/card_english_after_school"
        app:layout_constraintTop_toTopOf="@+id/textView20" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="@dimen/_36dp"
        android:layout_height="@dimen/_20dp"
        android:layout_marginStart="@dimen/_254dp"
        android:background="@drawable/shape_connect_line_right"
        app:layout_constraintBottom_toBottomOf="@+id/card_english_classic"
        app:layout_constraintStart_toStartOf="@+id/card_english_classic"
        app:layout_constraintTop_toTopOf="@+id/card_english_classic" />

    <FrameLayout
        android:id="@+id/card_english_classic"
        android:layout_width="@dimen/_265dp"
        android:layout_height="@dimen/_250dp"
        android:layout_marginTop="@dimen/_15dp"
        android:background="@mipmap/ic_chinese_classic"
        app:layout_constraintStart_toStartOf="@+id/card_english_prepare_read"
        app:layout_constraintTop_toBottomOf="@+id/card_english_prepare_read">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_marginStart="@dimen/_73dp"
            android:layout_marginBottom="@dimen/_18dp"
            android:text="经典巩固"
            android:textColor="@color/white"
            android:textSize="@dimen/_26sp" />
    </FrameLayout>

    <TextView
        android:id="@+id/textView20"
        android:layout_width="@dimen/_20dp"
        android:layout_height="@dimen/_36dp"
        android:layout_marginBottom="@dimen/_246dp"
        android:background="@drawable/shape_connect_line_bottom"
        app:layout_constraintBottom_toBottomOf="@+id/card_english_classic"
        app:layout_constraintEnd_toEndOf="@+id/card_english_classic"
        app:layout_constraintStart_toStartOf="@+id/card_english_classic" />

    <FrameLayout
        android:id="@+id/card_english_prepare_read"
        android:layout_width="@dimen/_538dp"
        android:layout_height="@dimen/_249dp"
        android:layout_marginStart="@dimen/_75dp"
        android:layout_marginTop="@dimen/_29dp"
        android:background="@mipmap/ic_chinese_prepare"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_marginStart="@dimen/_73dp"
            android:layout_marginBottom="@dimen/_18dp"
            android:text="课前预习"
            android:textColor="@color/white"
            android:textSize="@dimen/_26sp" />
    </FrameLayout>

    <LinearLayout
        android:id="@+id/textView41"
        android:layout_width="@dimen/_346dp"
        android:layout_height="@dimen/_548dp"
        android:layout_marginStart="@dimen/_51dp"
        android:background="@mipmap/ic_english_special_container_bg"
        android:orientation="vertical"
        android:paddingStart="@dimen/_18dp"
        android:paddingEnd="@dimen/_32dp"
        app:layout_constraintStart_toEndOf="@+id/card_english_prepare_read"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/btn_english_memorize_words"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_120dp"
            android:layout_marginTop="@dimen/_28dp"
            android:background="@drawable/shape_round_rect_white_alpha2"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="@dimen/_76dp"
                android:layout_height="@dimen/_76dp"
                android:layout_marginStart="@dimen/_23dp"
                android:src="@mipmap/ic_english_word" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24dp"
                android:layout_marginEnd="@dimen/_15dp"
                android:maxLines="1"
                android:singleLine="true"
                android:text="五维记单词"
                android:textColor="@color/white"
                android:textSize="@dimen/_26sp"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btn_english_read"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_120dp"
            android:layout_marginTop="@dimen/_8dp"
            android:background="@drawable/shape_round_rect_white_alpha2"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="@dimen/_76dp"
                android:layout_height="@dimen/_76dp"
                android:layout_marginStart="@dimen/_23dp"
                android:src="@mipmap/ic_english_read" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24dp"
                android:layout_marginEnd="@dimen/_15dp"
                android:maxLines="1"
                android:singleLine="true"
                android:text="英语阅读"
                android:textColor="@color/white"
                android:textSize="@dimen/_26sp"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btn_english_grammar"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_120dp"
            android:layout_marginTop="@dimen/_8dp"
            android:background="@drawable/shape_round_rect_white_alpha2"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="@dimen/_76dp"
                android:layout_height="@dimen/_76dp"
                android:layout_marginStart="@dimen/_23dp"
                android:src="@mipmap/ic_english_grammar" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24dp"
                android:layout_marginEnd="@dimen/_15dp"
                android:maxLines="1"
                android:singleLine="true"
                android:text="英语语法"
                android:textColor="@color/white"
                android:textSize="@dimen/_26sp"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btn_english_write"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_120dp"
            android:layout_marginTop="@dimen/_8dp"
            android:background="@drawable/shape_round_rect_white_alpha2"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="@dimen/_76dp"
                android:layout_height="@dimen/_76dp"
                android:layout_marginStart="@dimen/_23dp"
                android:src="@mipmap/ic_english_write" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24dp"
                android:layout_marginEnd="@dimen/_15dp"
                android:maxLines="1"
                android:singleLine="true"
                android:text="英语写作"
                android:textColor="@color/white"
                android:textSize="@dimen/_26sp"
                android:textStyle="bold" />
        </LinearLayout>

    </LinearLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/textView42"
        android:layout_width="@dimen/_148dp"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/_22dp"
        android:layout_marginEnd="@dimen/_76dp"
        android:orientation="vertical"
        android:paddingRight="@dimen/_14dp"
        android:gravity="center_horizontal"
        android:background="@mipmap/ic_english_tool_container_bg"
        app:layout_constraintBottom_toBottomOf="@+id/textView41"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView41"
        app:layout_constraintTop_toTopOf="@+id/textView41" >

        <ImageView
            android:id="@+id/btn_english_tool_translate"
            android:layout_width="@dimen/_74dp"
            android:layout_height="@dimen/_74dp"
            android:layout_marginTop="@dimen/_28dp"
            android:src="@mipmap/ic_english_tool_translate"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:includeFontPadding="false"
            android:text="中英翻译"
            android:textColor="@color/white"
            android:textSize="@dimen/_24sp"
            android:layout_marginTop="@dimen/_115dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/btn_english_tool_oral_test"
            android:layout_width="@dimen/_74dp"
            android:layout_height="@dimen/_74dp"
            android:layout_marginTop="@dimen/_158dp"
            android:src="@mipmap/ic_english_tool_oral_test"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView48"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="口语测评"
            android:textColor="@color/white"
            android:textSize="@dimen/_24sp"
            android:layout_marginTop="@dimen/_13dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btn_english_tool_oral_test" />

        <ImageView
            android:id="@+id/btn_english_tool_after_school"
            android:layout_width="@dimen/_74dp"
            android:layout_height="@dimen/_74dp"
            android:layout_marginBottom="@dimen/_185dp"
            android:src="@mipmap/ic_english_tool_after_school"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView53"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_13dp"
            android:text="课外英语"
            android:textColor="@color/white"
            android:textSize="@dimen/_24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btn_english_tool_after_school" />

        <ImageView
            android:id="@+id/btn_english_tool_error_book"
            android:layout_width="@dimen/_74dp"
            android:layout_height="@dimen/_74dp"
            android:layout_marginBottom="@dimen/_56dp"
            android:src="@mipmap/ic_english_tool_error_book"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView57"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="错题本"
            android:textColor="@color/white"
            android:textSize="@dimen/_24sp"
            android:layout_marginTop="@dimen/_13dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btn_english_tool_error_book" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>