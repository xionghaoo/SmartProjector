<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NumberKeyboardView">
        <attr name="nkv_width" format="dimension"/>
        <attr name="nkv_type" format="enum">
            <enum name="number" value="0"/>
            <enum name="phone" value="1"/>
            <enum name="cash" value="2"/>
            <enum name="pop" value="3"/>
            <enum name="screen_lock" value="4"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="RadarView">
        <attr name="rv_color" format="color"/>
    </declare-styleable>
</resources>