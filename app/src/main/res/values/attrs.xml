<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NumberKeyboardView">
        <attr name="nkv_width" format="dimension"/>
        <attr name="nkv_type" format="enum">
            <enum name="number" value="0"/>
            <enum name="phone" value="1"/>
            <enum name="cash" value="2"/>
            <enum name="pop" value="3"/>
            <enum name="screen_lock" value="4"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="RadarView">
        <attr name="rv_color" format="color"/>
    </declare-styleable>
    <declare-styleable name="NetworkStateLayout">
        <attr name="nsl_loading" format="reference"/>
        <attr name="nsl_error" format="reference"/>
        <attr name="nsl_empty" format="reference"/>
    </declare-styleable>
    <declare-styleable name="SettingsMenuItemView">
        <attr name="smiv_title" format="string"/>
    </declare-styleable>

    <declare-styleable name="SystemCardView">
        <attr name="scv_systemType" format="enum">
            <enum name="infant" value="0"/>
            <enum name="elementary" value="1"/>
            <enum name="secondary" value="2"/>
        </attr>
    </declare-styleable>
</resources>